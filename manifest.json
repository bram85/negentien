{
  "name": "Negentien",
  "version": "0.3.3",
  "manifest_version": 2,
  "description": "Zoekt woorden van 19 letters in Nederlandstalige pagina's.",
  "background": {
    "scripts": [ "background.js" ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": { "19": "icon19.png", "38": "icon38.png" },
    "default_title": "Negentien"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://*/*",
    "tabs"
  ]
}
